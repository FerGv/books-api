{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "airbnb-typescript/base"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "project": ["tsconfig.json"],
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "@typescript-eslint/lines-between-class-members": "off"
  }
}
